      <Accordion fx:id="acAccordion">
        <panes>
          <TitledPane fx:id="tpStatistik" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" text="Statistik">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <PieChart fx:id="pcChart" layoutX="14.0" layoutY="14.0" prefHeight="300.0" prefWidth="350.0" />
                     </children>
                  </AnchorPane>
               </content>
            </TitledPane>
        </panes>
      </Accordion>
   </children>
</AnchorPane>
